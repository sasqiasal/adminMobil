<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginBottom="20dp"
    tools:context=".AkunProfile">

    <RelativeLayout
        android:id="@+id/inti"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/rounded_bgputih"
        app:layout_constraintTop_toTopOf="parent">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/syarat"
                android:layout_width="40dp"
                android:layout_height="40dp"
                android:layout_marginLeft="10dp"
                android:layout_marginTop="240dp"
                android:src="@drawable/icon_directions" />

            <ImageView
                android:id="@+id/btnsyaratdnaketentuan"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:layout_alignParentEnd="true"
                android:layout_marginLeft="350dp"
                android:layout_marginTop="240dp"
                android:layout_marginEnd="31dp"
                android:src="@drawable/icon_arrow_forward" />

            <TextView
                android:id="@+id/ketentuan"
                android:layout_width="240dp"
                android:layout_height="wrap_content"
                android:layout_marginStart="60dp"
                android:layout_marginLeft="60dp"
                android:layout_marginTop="255dp"
                android:layout_marginEnd="0dp"
                android:layout_marginBottom="475dp"
                android:text="Syarat dan Ketentuan" />

            <View
                android:id="@+id/view5"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginTop="290dp"
                android:background="#000000" />

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="295dp">

                <ImageView
                    android:id="@+id/pusat"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="180dp"
                    android:src="@drawable/icon_chat" />

                <ImageView
                    android:id="@+id/btnpusatbantuan"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginLeft="350dp"
                    android:layout_marginTop="180dp"
                    android:layout_marginEnd="31dp"
                    android:src="@drawable/icon_arrow_forward" />

                <TextView
                    android:id="@+id/bantuan"
                    android:layout_width="240dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="60dp"
                    android:layout_marginLeft="60dp"
                    android:layout_marginTop="190dp"
                    android:layout_marginEnd="0dp"
                    android:layout_marginBottom="475dp"
                    android:text="Pusat Bantuan" />

                <View
                    android:id="@+id/view"
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="230dp"
                    android:background="#000000" />

                <RelativeLayout
                    android:id="@+id/aboutMitra"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                    <ImageView
                        android:id="@+id/mitra"
                        android:layout_width="40dp"
                        android:layout_height="40dp"
                        android:layout_marginLeft="10dp"
                        android:layout_marginTop="120dp"
                        android:src="@drawable/icon_info" />

                    <ImageView
                        android:id="@+id/btntentangmitra"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginLeft="350dp"
                        android:layout_marginTop="120dp"
                        android:layout_marginEnd="31dp"
                        android:src="@drawable/icon_arrow_forward" />


                    <TextView
                        android:id="@+id/about"
                        android:layout_width="240dp"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="60dp"
                        android:layout_marginLeft="60dp"
                        android:layout_marginTop="130dp"
                        android:layout_marginEnd="0dp"
                        android:layout_marginBottom="475dp"
                        android:text="Tentang Mitra Sempurna Jaya" />

                    <View
                        android:layout_width="match_parent"
                        android:layout_height="1dp"
                        android:layout_marginTop="170dp"
                        android:background="#000000" />
                </RelativeLayout>
            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/ubahdata"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <ImageView
                    android:id="@+id/akun"
                    android:layout_width="40dp"
                    android:layout_height="40dp"
                    android:layout_marginLeft="10dp"
                    android:layout_marginTop="50dp"
                    android:src="@drawable/icon_account" />

                <ImageView
                    android:id="@+id/btndataakun"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:layout_alignParentEnd="true"
                    android:layout_marginLeft="350dp"
                    android:layout_marginTop="50dp"
                    android:layout_marginEnd="31dp"
                    android:src="@drawable/icon_arrow_forward" />

                <TextView
                    android:id="@+id/ubah"
                    android:layout_width="224dp"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="60dp"
                    android:layout_marginLeft="60dp"
                    android:layout_marginTop="60dp"
                    android:layout_marginBottom="545dp"
                    android:text="Data Akun" />

                <View
                    android:layout_width="match_parent"
                    android:layout_height="1dp"
                    android:layout_marginTop="100dp"
                    android:background="#000000" />


            </RelativeLayout>


        </RelativeLayout>


    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>